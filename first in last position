#using binary search

class Solution:
    def searchRange(self, nums: List[int], target: int) -> List[int]:
        left = 0
        right = len(nums) - 1
        while(left <= right):
            mid = (left + right) // 2
        if (arr[mid] > target):
            right = mid - 1
        elif(arr[mid] < target):
            left = mid + 1
            if(mid == 0):
                return mid
            elif(arr[mid - 1] != key):
                return mid

        return mid + 1
        
        
        # using linear search
        class Solution:
    def searchRange(self, nums: List[int], target: int) -> List[int]:
        first=-1
        last=-1
        for i in range(len(nums)):
            if(nums[i]==target):
                if(first==-1):
                    first=i
                    last=i
                else:
                    last=i
        return (first,last)    
        
