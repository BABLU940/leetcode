class Solution:
    def maxProfit(self, prices: List[int]) -> int:
        b, s = 0, 1  #b:buyer ,s: seller
        maxP = 0
        
        while s < len(prices):
            if prices[b] < prices[s]:
                profit = prices[s] - prices[b]
                maxP = max(maxP, profit)
            else:
                b = s
            s += 1
        return maxP       
        
